{
  "name": "stripe-terminal-react-native-example",
  "description": "Example app for stripe-terminal-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:all": "concurrently \"yarn start\" \"yarn start:server\" \"yarn android\"",
    "bootstrap": "cd .. && yarn bootstrap",
    "build:server": "babel server --out-dir dist --extensions '.ts,.tsx' --ignore '**/__tests__/**' --source-maps --copy-files --delete-dir-on-start",
    "ios": "react-native run-ios",
    "ios:all": "concurrently \"yarn start\" \"yarn start:server\" \"yarn ios\"",
    "postinstall": "scripts/fixRN.sh",
    "start": "react-native start",
    "start:server": "ts-node --project ./tsconfig.server.json server/index.ts",
    "start:server:ci": "yarn start:server &",
    "watch:server": "nodemon --exec yarn start:server"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "^2.1.0",
    "@react-navigation/native": "^5.x",
    "@react-navigation/stack": "^5.x",
    "@types/jest": "^27.0.2",
    "@types/react-native-dotenv": "^0.2.0",
    "dotenv": "^10.0.0",
    "jest": "^27.3.1",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-config": "^1.4.5",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "2.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@types/express": "^4.17.13",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^7.0.0",
    "detox": "^19.4.3",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "nodemon": "^2.0.15",
    "stripe": "^8.181.0",
    "ts-node": "^10.4.0",
    "winston": "^3.3.3"
  }
}
