{
  "testRunner": {
    "$0": "jest",
    "args": {
      "config": "e2e/jest.config.js"
    }
  },
  "apps": {
    "ios.example.debug": {
      "type": "ios.app",
      "binaryPath": "example-app/ios/build/Build/Products/Debug-iphonesimulator/StripeTerminalReactNativeExampleApp.app",
      "build": "xcodebuild -workspace example-app/ios/exampleapp.xcworkspace -configuration Debug -scheme exampleapp -destination 'platform=iOS Simulator,name=iPhone SE (3rd generation)' -derivedDataPath example-app/ios/build"
    },
    "android.example.debug": {
      "type": "android.apk",
      "binaryPath": "example-app/android/app/build/outputs/apk/debug/app-debug.apk",
      "build": "cd example-app/android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug ; cd -"
    },
    "ios.debug": {
      "type": "ios.app",
      "binaryPath": "dev-app/ios/build/Build/Products/Debug-iphonesimulator/StripeTerminalReactNativeDevApp.app",
      "build": "xcodebuild -workspace dev-app/ios/StripeTerminalReactNativeDevApp.xcworkspace -configuration Debug -scheme StripeTerminalReactNativeDevApp -destination 'platform=iOS Simulator,name=iPhone SE (3rd generation)' -derivedDataPath dev-app/ios/build"
    },
    "ios.release": {
      "type": "ios.app",
      "binaryPath": "dev-app/ios/build/Build/Products/Release-iphonesimulator/StripeTerminalReactNativeDevApp.app",
      "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace dev-app/ios/StripeTerminalReactNativeDevApp.xcworkspace -configuration Release -scheme StripeTerminalReactNativeDevApp -destination 'platform=iOS Simulator,name=iPhone SE (3rd generation)' -derivedDataPath dev-app/ios/build"
    },
    "android.debug": {
      "type": "android.apk",
      "binaryPath": "dev-app/android/app/build/outputs/apk/debug/app-debug.apk",
      "build": "cd dev-app/android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug ; cd -"
    },
    "android.release": {
      "type": "android.apk",
      "binaryPath": "dev-app/android/app/build/outputs/apk/release/app-release.apk",
      "build": "export RCT_NO_LAUNCH_PACKAGER=true && (cd dev-app/android; ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release ; cd -)"
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 14"
      }
    },
    "emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "Nexus_6_API_34_2",
        "utilBinaryPaths": ["./test-butler-app.apk"]
      }
    },
    "ci-emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "TestingAVD"
      },
      "utilBinaryPaths": ["./test-butler-app.apk"]
    }
  },
  "configurations": {
    "ios.example": {
      "device": "simulator",
      "app": "ios.example.debug"
    },
    "android.example": {
      "device": "ci-emulator",
      "app": "android.example.debug"
    },
    "ios": {
      "device": "simulator",
      "app": "ios.debug"
    },
    "ios.sim.release": {
      "device": "simulator",
      "app": "ios.release"
    },
    "android": {
      "device": "emulator",
      "app": "android.debug"
    },
    "android.emu.release": {
      "device": "ci-emulator",
      "app": "android.release"
    }
  }
}
