{
  "name": "@stripe/stripe-terminal-react-native-example-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "build:android:ci": "expo run:android --no-bundler",
    "eject": "expo eject",
    "ios": "expo run:ios",
    "postinstall": "../dev-app/scripts/fixRN.sh",
    "start": "expo start --dev-client",
    "typescript": "tsc --noEmit",
    "web": "expo start --web"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "react-native-reanimated"
      ]
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@stripe/stripe-terminal-react-native": "^0.0.1-beta.25",
    "@types/jest": "^29.2.1",
    "expo": "^53.0.0",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "jest": "^29.2.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "^0.79.0",
    "react-native-config": "^1.4.5",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "^3.17.5",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-web": "^0.20.0"
  },
  "resolutions": {
    "@types/react": "17.0.41"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/cli-platform-android": "^18.0.0",
    "@react-native-community/cli-platform-ios": "^18.0.0",
    "@react-native/metro-config": "^0.79.0",
    "@rnx-kit/align-deps": "^3.0.8",
    "@types/react": "~19.0.10",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "jest": "^29.2.1",
    "stripe": "^8.181.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.79"
      ],
      "capabilities": [
        "animation",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-dom",
        "safe-area",
        "screens",
        "storage"
      ]
    }
  }
}
