{
  "name": "@stripe/stripe-terminal-react-native-example-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "build:android:ci": "expo run:android --no-bundler",
    "eject": "expo eject",
    "ios": "expo run:ios",
    "postinstall": "../dev-app/scripts/fixRN.sh",
    "start": "expo start --dev-client",
    "typescript": "tsc --noEmit",
    "web": "expo start --web"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "react-native-reanimated"
      ]
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.3.21",
    "@stripe/stripe-terminal-react-native": "^0.0.1-beta.25",
    "@types/jest": "^29.2.1",
    "expo": "^52.0.0",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "jest": "^29.2.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.0",
    "react-native-config": "^1.4.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-root-toast": "^3.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "resolutions": {
    "@types/react": "17.0.41"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-community/cli": "^15.0.1",
    "@react-native-community/cli-platform-android": "^15.0.1",
    "@react-native-community/cli-platform-ios": "^15.0.1",
    "@react-native/metro-config": "^0.76.0",
    "@rnx-kit/align-deps": "^3.0.8",
    "@types/react": "~18.3.12",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "jest": "^29.2.1",
    "stripe": "^8.181.0",
    "typescript": "~5.3.3"
  },
  "private": true,
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.76"
      ],
      "capabilities": [
        "animation",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-dom",
        "safe-area",
        "screens",
        "storage"
      ]
    }
  }
}
